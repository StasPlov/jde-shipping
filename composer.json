{
	"name": "stas-plov/jde-shipping",
	"version": "v1.0.0",
	"type": "library",
	"license": "MIT",
	"description": "Adds validator for middleware request support in your Symfony application",
	"keywords": [
		"Shipping",
		"php",
		"api",
		"jde",
		"api",
		"rest"
	],
	"authors": [
		{
			"name": "Stas Plov",
			"email": "SaviouR.S@mail.ru",
			"homepage": "https://github.com/StasPlov",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/StasPlov/jde-shipping/issues"
	},
	"require": {
		"php": "^7.4 || ^8.2",
		"ext-json": "*",
		"jms/serializer": "^3.30",
		"symfony/http-client": "^7.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.3",
		"psr/log": "^1.0 || ^2.0 || ^3.0"
	},
	"autoload": {
		"psr-4": {
			"JdeShipping\\": "src/"
		},
		"exclude-from-classmap": [
			"/tests/"
		]
	},
	"config": {
		"allow-plugins": true,
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-main": "1.x-dev"
		}
	}
}